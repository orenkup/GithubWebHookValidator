<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.0.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Convert JSON encoded webhooks from Github into an easy to use C# object</Description>
    <Copyright>Promofaux 2018</Copyright>
    <PackageLicenseUrl>https://github.com/PromoFaux/GithubWebhook/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/PromoFaux/GithubWebhook</PackageProjectUrl>
    <RepositoryUrl>https://github.com/PromoFaux/GithubWebhook</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Http" Version="2.0.1" />
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>

</Project>
